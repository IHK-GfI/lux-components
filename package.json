{
  "name": "@ihk-gfi/lux-components",
  "version": "10.5.0",
  "license": "IHK-GfI Open-Source-Lizenz",
  "repository": {
    "type": "git",
    "url": "https://github.com/IHK-GfI/lux-components.git"
  },
  "homepage": "https://github.com/IHK-GfI/lux-components",
  "bugs": {
    "url": "https://github.com/IHK-GfI/lux-components/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-ie": "ng serve --configuration es5 --port 4201",
    "build": "ng build --source-map",
    "build-aot": "ng build --aot",
    "test": "ng test",
    "test_no_sm": "ng test --no-sourceMap",
    "test_single_run": "ng test --watch=false --browsers=ChromeHeadless",
    "test_coverage": "ng test --watch=false --code-coverage=true",
    "lint": "ng run lux-components:app-lint --format=stylish && ng run lux-components:spec-lint --format=stylish",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "doc": "compodoc -p src/tsconfig.app.json -n 'LUX-Components'",
    "docStart": "compodoc src -s --port 8888",
    "security": "npm audit --registry=https://registry.npmjs.org --audit-level high",
    "smoketest": "npm run test_single_run && npm run build-aot && npm run lint --bailOnLintError true"
  },
  "private": false,
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.6",
    "@angular/animations": "^11.0.7",
    "@angular/cdk": "^10.2.7",
    "@angular/cli": "^11.0.6",
    "@angular/common": "^11.0.7",
    "@angular/compiler": "^11.0.7",
    "@angular/compiler-cli": "^11.0.7",
    "@angular/core": "^11.0.7",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "^11.0.7",
    "@angular/language-service": "^11.0.7",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^11.0.7",
    "@angular/platform-browser-dynamic": "^11.0.7",
    "@angular/router": "^11.0.7",
    "@compodoc/compodoc": "^1.1.11",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.20",
    "codelyzer": "^6.0.0",
    "core-js": "^3.8.2",
    "dompurify": "^2.2.6",
    "hammerjs": "^2.0.8",
    "html-webpack-plugin": "^4.5.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-viewport": "^1.0.7",
    "marked": "^1.2.7",
    "material-design-icons-iconfont": "^5.0.1",
    "ng-packagr": "^11.0.3",
    "ng2-pdf-viewer": "^6.3.2",
    "node-sass": "^4.14.1",
    "pdfjs-dist": "^2.5.207",
    "prettier": "^2.2.1",
    "protractor": "^7.0.0",
    "rxjs": "^6.6.3",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.0",
    "tslint": "^6.1.3",
    "tslint-angular": "^3.0.3",
    "typescript": "^4.0.5",
    "zone.js": "^0.10.3"
  }
}
