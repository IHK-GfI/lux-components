{
  "name": "@ihk-gfi/lux-components",
  "version": "14.4.0",
  "license": "IHK-GfI Open-Source-Lizenz",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/IHK-GfI/lux-components.git"
  },
  "homepage": "https://github.com/IHK-GfI/lux-components",
  "bugs": {
    "url": "https://github.com/IHK-GfI/lux-components/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-en": "ng serve --configuration en --port 4210",
    "build": "ng build --source-map",
    "build-prod": "ng build --configuration production --localize --single-bundle && npm run move-de-files",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless --code-coverage=true",
    "lint": "ng lint --fix --max-warnings=0",
    "packagr": "ng-packagr -p ng-package.json --config tsconfig.lib.json",
    "security": "npm audit --registry=https://registry.npmjs.org --audit-level high",
    "smoketest": "npm run build-prod && npm run lint && npm run test-headless && npm run xi18n && npm run packagr",
    "move-de-files": "node move-de-files.js",
    "xi18n": "ng extract-i18n --output-path src/locale",
    "husky-init": "husky install"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.10",
    "@angular-eslint/builder": "14.3.0",
