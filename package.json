  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-ie": "ng serve --configuration es5 --port 4201",
    "build": "ng build --source-map",
    "build-aot": "ng build --aot",
    "test": "ng test",
    "test_no_sm": "ng test --no-sourceMap",
    "test_single_run": "ng test --watch=false --browsers=ChromeHeadless",
    "test_coverage": "ng test --watch=false --code-coverage=true",
    "lint": "ng run lux-components:app-lint --format=stylish && ng run lux-components:spec-lint --format=stylish",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "doc": "compodoc -p src/tsconfig.app.json -n 'LUX-Components'",
    "docStart": "compodoc src -s --port 8888",
    "security": "npm audit --registry=https://registry.npmjs.org --audit-level high",
    "smoketest": "npm run test_single_run && npm run build-aot && npm run lint --bailOnLintError true"
  },
  "private": false,
